    "escenas": [
      {
        "nombre": "escena_de_juego",
        "codigo": "class escena_de_juego extends Escena {\n    iniciar() {\n        // Al comenzar el juego clona varias veces el bloque\n        // de color verde.\n\n        for (let fila = 0; fila < 3; fila++) {\n            for (let columna = 0; columna < 6; columna++) {\n                this.pilas.clonar_en(\"bloque\", -180 + columna * 70, 100 + fila * 40)\n            }\n        }\n\n    }\n\n    actualizar() {\n\n    }\n\n    // Se llama cuando el actor pelota sale de la pantalla\n    // y envía el mensajes \"perdio\" con la función\n    // this.pilas.enviar_mensaje_global(\"perdio\").\n    cuando_llega_el_mensaje_perdio() {\n        this.pilas.clonar(\"texto_has_perdido\");\n        this.pilas.clonar(\"reiniciar_escena\");\n\n        let jugador = this.pilas.obtener_actor_por_nombre(\"jugador\");\n        jugador.eliminar();\n\n        this.pilas.clonar_en(\"explosion\", jugador.x, jugador.y);\n        this.pilas.camara.vibrar(2, 0.25)\n    }\n\n    // Se llama cuando el actor pelota logra eliminar un bloque\n    // y envía el mensajes \"elimina_bloque\" con la función\n    // this.pilas.enviar_mensaje_global(\"elimina_bloque\").\n    cuando_llega_el_mensaje_elimina_bloque() {\n        let cantidad = this.pilas.obtener_todos_los_actores_con_la_etiqueta(\"bloque\").length;\n\n        if (cantidad === 0) {\n            let pelota = this.pilas.obtener_actor_por_nombre(\"pelota_azul\");\n            pelota.eliminar();\n            this.pilas.clonar(\"texto_gana\")\n        }\n    }\n}"
      },
