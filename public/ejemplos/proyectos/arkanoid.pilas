    "actores": [
      {
        "nombre": "plataforma",
        "codigo": "// @ts-ignore\nclass plataforma extends Actor {\n    iniciar() {}\n}"
      },
      {
        "nombre": "caja",
        "codigo": "// @ts-ignore\nclass caja extends Actor {\n    iniciar() {}\n}"
      },
      {
        "nombre": "pelota_azul",
        "codigo": "// @ts-ignore\nclass pelota_azul extends Actor {\n    iniciar() {}\n\n    // Este método se llama automáticamente\n    // cuando el jugador pulsa la tecla ESPACIO.\n    // Mirá el código de jugar y buscá la llamada\n    // a la función pelota.enviar_mensaje(\"empujar\")\n    cuando_llega_el_mensaje_empujar() {\n        this.velocidad_x = 5;\n        this.velocidad_y = 5;\n    }\n\n    actualizar() {\n        //realiza el movimiento de la pelota.\n        this.x += this.velocidad_x;\n        this.y += this.velocidad_y;\n\n        this.actualizar_posicion_para_no_salir_de_la_pantalla();\n        this.actualizar_colision_con_el_jugador();\n        this.verificar_si_el_jugador_perdio();\n    }\n\n    actualizar_posicion_para_no_salir_de_la_pantalla() {\n        // Evita que salga por el borde derecho.\n        if (this.x > 240) {\n            this.x = 240;\n            this.velocidad_x *= -1;\n        }\n\n        // Evita que salga por el borde izquierdo.\n        if (this.x < -240) {\n            this.x = -240;\n            this.velocidad_x *= -1;\n        }\n\n        // Evita que salga por arriba.\n        if (this.y > 240) {\n            this.y = 240;\n            this.velocidad_y *= -1;\n        }\n    }\n\n    actualizar_colision_con_el_jugador() {\n        // Si la pelota está por colisionar con el jugador, se\n        // intenta verificar si el jugador está en posición\n        // de volver a empujar la pelota hacia arriba o no.\n\n        if (this.y < -200 && this.y > -250) {\n            let jugador = this.pilas.obtener_actor_por_nombre(\"jugador\");\n\n            let borde_izquierdo_del_jugador = jugador.x - 50;\n            let borde_derecho_del_jugador = jugador.x + 50;\n\n            // si está colisionando con el jugador.\n            if (this.x > borde_izquierdo_del_jugador && this.x < borde_derecho_del_jugador) {\n                // coloca la pelota justo en contacto con el jugador.\n                this.y = -200;\n\n                this.velocidad_y *= -1;\n\n                // elije la velocidad_x en base a dónde colisionó\n                // con el jugador usando una regla de 3 simple.\n                let diferencia = this.x - jugador.x;\n                this.velocidad_x = (diferencia * 10) / 50;\n            }\n\n        }\n    }\n\n    verificar_si_el_jugador_perdio() {\n        if (this.y < -250) {\n            this.eliminar();\n            this.enviar_mensaje_global(\"perdio\");\n        }\n    }\n\n    cuando_colisiona(actor: Actor) {\n        if (actor.tiene_etiqueta(\"bloque\")) {\n            let izquierda_del_bloque = actor.x - 31;\n            let derecha_del_bloque = actor.x + 31;\n\n            // Si colisiona por abajo o arriba del bloque se empuja a la pelota\n            // verticalmente\n            if (izquierda_del_bloque < this.x && this.x < derecha_del_bloque) {\n                actor.eliminar();\n                this.velocidad_y *= -1;\n            } else {\n                actor.eliminar();\n                // en caso contrario, el golpe se tendría que haber dado\n                // horizontalmente, así que se empuja a la pelota horizontalmente.\n                this.velocidad_x *= -1;\n            }\n\n\n            // Emite un mensaje global para avisale al actor\n            // texto_bloques que vuelva a calcular cuantos\n            // bloques hay en el escenario.\n            //\n            // Este mensaje también lo recibe la escena\n            // del juego, y la utiliza para saber si tiene\n            // que decirle al jugador si pasó de nivel.\n            this.enviar_mensaje_global(\"elimina_bloque\");\n\n        }\n    }\n}"
      },
