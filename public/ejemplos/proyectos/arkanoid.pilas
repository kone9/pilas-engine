      {
        "nombre": "jugador",
        "codigo": "// @ts-ignore\nclass jugador extends Actor {\n\n    fase = \"comenzando\";\n\n    iniciar() {\n\n    }\n\n    actualizar() {\n        let velocidad = 8;\n\n        // permite mover el jugador de izquierda a derecha.\n        if (this.control.izquierda) {\n            this.x -= velocidad;\n        }\n\n        if (this.control.derecha) {\n            this.x += velocidad;\n        }\n\n        // limita el movimiento para que no salga de la pantalla.\n        if (this.x < -200) {\n            this.x = -200;\n        }\n\n        if (this.x > 200) {\n            this.x = 200;\n        }\n\n        // en la fase inicial, la pelota debe seguir el\n        // movimiento del jugador.\n        if (this.fase === \"comenzando\") {\n            let pelota = this.pilas.obtener_actor_por_nombre(\"pelota_azul\");\n            pelota.x = this.x;\n        }\n    }\n\n    cuando_pulsa_tecla(tecla) {\n        // Permite disparar la pelota cuando se pulsa la tecla espacio.\n        if (this.fase === \"comenzando\") {\n            if (tecla === \"espacio\") {\n                let pelota = this.pilas.obtener_actor_por_nombre(\"pelota_azul\");\n\n                pelota.enviar_mensaje(\"empujar\");\n\n                this.fase = \"jugando\";\n                this.eliminar_cartel_inicial();\n            }\n        }\n    }\n\n    eliminar_cartel_inicial() {\n        let cartel = this.pilas.obtener_actor_por_nombre(\"cartel\");\n        cartel.eliminar();\n    }\n}"
